{
    "name": "mui-layout-framework",
    "version": "1.1.0",
    "description": "A framework for creating consistent and appealing pages with Material-UI.",
    "author": "Reece Dunham",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/RDIL/mui-layout-framework"
    },
    "main": "dist/index.js",
    "module": "dist/index.modern.js",
    "source": "src/index.tsx",
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "build": "microbundle-crl --no-compress --format modern,cjs && rimraf dist/stories",
        "start": "microbundle-crl watch --no-compress --format modern,cjs",
        "prepublish": "run-s build",
        "prettier": "prettier --write **/*.{js,jsx,json,ts,tsx,css,md,html}",
        "test": "run-s test:build test:lint test:jest",
        "test:build": "run-s build",
        "test:lint": "eslint .",
        "test:jest": "jest",
        "storybook": "start-storybook -p 6006 --no-dll",
        "storybook:build": "build-storybook --no-dll"
    },
    "peerDependencies": {
        "@material-ui/core": ">=4.6.0",
        "react": ">=16.4.0 || 17.x",
        "react-helmet": "6.x"
    },
    "devDependencies": {
        "@babel/core": "^7.12.3",
        "@babel/preset-env": "^7.12.1",
        "@babel/preset-react": "^7.12.5",
        "@babel/preset-typescript": "^7.12.1",
        "@material-ui/core": "^4.11.0",
        "@storybook/addon-viewport": "^6.0.28",
        "@storybook/react": "^6.0.28",
        "@types/jest": "^26.0.15",
        "@types/react": "^16.9.55",
        "@types/react-dom": "^16.9.9",
        "@types/react-helmet": "^6.1.0",
        "@types/react-test-renderer": "^16.9.3",
        "@typescript-eslint/eslint-plugin": "^4.7.0",
        "@typescript-eslint/parser": "^4.7.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.6.3",
        "babel-loader": "^8.2.1",
        "cross-env": "^7.0.2",
        "eslint": "^7.13.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^24.1.3",
        "eslint-plugin-react": "^7.21.5",
        "jest": "^26.6.3",
        "microbundle-crl": "^0.13.11",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.1.2",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-is": "^17.0.1",
        "react-test-renderer": "^17.0.1",
        "rimraf": "^3.0.2",
        "typescript": "^4.0.5"
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "react-helmet": "^6.1.0"
    },
    "keywords": [
        "material",
        "ui",
        "mui",
        "layout",
        "framework"
    ]
}
